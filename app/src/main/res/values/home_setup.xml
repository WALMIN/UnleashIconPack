<?xml version="1.0" encoding="utf-8"?><!--suppress CheckTagEmptyBody -->
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <!--
        Home actions style ==> Share / Rate / Donate buttons
        0. Hidden
        1. Small
        2. Big
    -->
    <integer name="home_actions_style">2</integer>

    <!--
        Do you want to show overview in home section?
        True, means yes.
        False, means no.
    -->
    <bool name="show_overview">true</bool>

    <!--
        Do you want the quick apply button to show its text?
        True, means yes.
        False, means no.
    -->
    <bool name="show_quick_apply_text">false</bool>

    <!--
        Set a custom text to the quick apply button.
        Be sure to add translations, or it might not be shown in other languages.
        Leave empty for the app to use the default "Quick Apply" text
    -->
    <string name="quick_apply_custom_text"></string>

    <!-- These arrays are for the list of apps or links in main section. -->
    <string-array name="home_list_titles">
        <item>Source code</item>
        <item>My other apps</item>
        <item>Official website</item>
    </string-array>
    <string-array name="home_list_descriptions">
        <item>The source code for the icon pack is over on GitHub</item>
        <item>Download my other apps from Google Play Store</item>
        <item>Check out the official website for more info</item>
    </string-array>
    <string-array name="home_list_icons">
        <item>github</item>
        <item>google_play_store</item>
        <item>web</item>
    </string-array>
    <string-array name="home_list_links">
        <item>https://github.com/WALMIN/unleashiconpack</item>
        <item>https://play.google.com/store/apps/dev?id=6020993160698769181</item>
        <item>https://walmin.com/unleashiconpack</item>
    </string-array>
</resources>